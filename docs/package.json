{
    "type": "module",
    "scripts": {
      "docs:dev": "vitepress dev",
      "docs:build": "vitepress build",
      "docs:preview": "vitepress preview",
      "docs:update:version": "node ./scripts/process-axios-version.js",
      "docs:update:sponsors": "node ./scripts/process-sponsors.js",
      "prod:build": "npm run docs:update:version && npm run docs:update:sponsors && npm run docs:build",
      "postinstall": "patch-package --exclude nothing"
    },
    "dependencies": {
      "@splidejs/splide": "^4.1.4",
      "patch-package": "^8.0.0",
      "vitepress": "^1.5.0"
    },
    "devDependencies": {
      "axios": "^1.7.9",
      "chalk": "^5.3.0",
      "dayjs": "^1.11.13",
      "xo": "^0.60.0"
    },
    "xo": {
      "space": true,
      "rules": {
        "object-curly-spacing": [
          "error",
          "always"
        ]
      }
    }
  }